{
  "mqtt": {
    "host": "mqtt://mqtt.airbits.com.ar",
    "port": 1883
  },
  "websocket": {
    "port": 80,
    "ssl_key": "privatekey.pem",
    "ssl_cert": "certificate.pem"
  },
  "protocolId": "MQIsdp",
  "protocolVersion": 3,
  "log4js": {
    "appenders": [
    	{
        "type": "console"
      },
      {
        "type": "file",
        "filename": "mqttwsBridge.log",
        "maxLogSize": 1024,
        "backups": 3
      }
    ]
  }
}

